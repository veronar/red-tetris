{
  "name": "red_tetrisboilerplate",
  "version": "0.0.1",
  "author": "redpelicans",
  "license": "MIT",
  "scripts": {
    "start": "concurrently --kill-others \"npm run start-client\" \"npm run start-srv\"",
    "eslint": "eslint src",
    "prod-client": "npx react-scripts build",
    "start-client": "npx react-scripts start",
    "start-srv": "npx nodemon src/server/index.js",
    "test": "react-scripts test",
    "coverage": "npm test -- --coverage --watchAll=false -u"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "express": "^4.17.3",
    "fontsource-roboto": "^2.2.6",
    "nodemon": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "styled-components": "^5.1.1",
    "yarn": "^1.22.4"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.4.1",
    "braces": "^3.0.2",
    "concurrently": "^5.2.0",
    "lodash": "^4.17.19",
    "nyc": "^15.1.0"
  },
  "nyc": {
    "exclude": [
      "build"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/server/index.js",
      "!src/index.js",
      "!src/client/components/propComponents.js",
      "!src/client/components/basicComponents.js",
      "!src/client/reducers/index.js"
    ]
  }
}
